{
  "name": "omdcmobile",
  "version": "0.9.7.4",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios  --simulator='iPhone 15'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod:m1": "cd ios && arch -x86_64 pod install && cd ..",
    "postinstall": "patch-package",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:aab": "cd android && ./gradlew bundleProdRelease && cd ..",
    "build:dev": "cd android && ./gradlew assembleDevRelease && cd ..",
    "deploycp:android": "appcenter codepush release-react -a omdcteam-gmail.com/OMDC-Mobile-1 -d Production",
    "deploycp:ios": "appcenter codepush release-react -a omdcteam-gmail.com/OMDC-Mobile -d Production",
    "android:dev": "react-native run-android --variant=devDebug",
    "android:prodDebug": "react-native run-android --variant=prodDebug"
  },
  "dependencies": {
    "@likashefqet/react-native-image-zoom": "^3.0.0",
    "@react-native-firebase/app": "^18.9.0",
    "@react-native-firebase/messaging": "^18.9.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "appcenter": "5.0.1",
    "appcenter-analytics": "5.0.1",
    "appcenter-crashes": "5.0.1",
    "axios": "^1.6.7",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.6.0",
    "moment": "^2.30.1",
    "npm": "^10.5.0",
    "patch-package": "^8.0.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-blob-util": "^0.19.1",
    "react-native-calendars": "^1.1303.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-exit-app": "^2.0.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-image-base64": "^0.1.4",
    "react-native-image-picker": "^7.1.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-images-to-pdf": "^0.2.1",
